{
  "name": "@mango-scripts/esp-config",
  "version": "1.0.1",
  "description": "A collection of configuration files containing eslint, stylelint, prettier",
  "keywords": [
    "eslint",
    "stylelint",
    "prettier",
    "config"
  ],
  "homepage": "https://github.com/AlbertLin0923/mango-scripts/tree/main/packages/esp-config",
  "bugs": {
    "url": "https://github.com/AlbertLin0923/mango-scripts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertLin0923/mango-scripts",
    "directory": "packages/esp-config"
  },
  "license": "MIT",
  "author": "AlbertLin0923",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./stylelint": {
      "types": "./dist/esm/stylelint.d.ts",
      "import": "./dist/esm/stylelint.mjs",
      "require": "./dist/cjs/stylelint.js"
    },
    "./prettier": {
      "types": "./dist/esm/prettier.d.ts",
      "import": "./dist/esm/prettier.mjs",
      "require": "./dist/cjs/prettier.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "postcss-less": "^6.0.0",
    "postcss-styl": "^0.12.3",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^6.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
