      "types": "./dist/esm/prettier.d.ts",
      "import": "./dist/esm/prettier.mjs",
      "require": "./dist/cjs/prettier.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild",
    "prepublishOnly": "pnpm run build"
